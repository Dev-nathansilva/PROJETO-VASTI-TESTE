{
  "rewrites": [
    {
      "source": "/home",
      "destination": "/index.html"
    },
    {
      "source": "/contato",
      "destination": "/contato.html"
    },
    {
      "source": "/colecao",
      "destination": "/colecao.html"
    }
  ],
  "redirects": [
    {
      "source": "/:slug",
      "has": [
        {
          "type": "query",
          "key": "slug",
          "value": "(?!home|contato|colecao)"
        }
      ],
      "destination": "/",
      "permanent": true
    }
  ]
}
