{
  "rewrites": [
    {
      "source": "/home",
      "destination": "/index.html"
    },
    {
      "source": "/contato",
      "destination": "/contato.html"
    }
  ],
  "redirects": [
    {
      "source": "/(?!home|contato).*",
      "destination": "/teste",
      "statusCode": 301
    }
  ]
}
