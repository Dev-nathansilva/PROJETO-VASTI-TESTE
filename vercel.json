{
  "rewrites": [
    {
      "source": "/home",
      "destination": "/index.html"
    },
    {
      "source": "/contato",
      "destination": "/contato.html"
    }
  ],
  "redirects": [
    {
      "source": "/:slug",
      "destination": "/",
      "has": [
        {
          "type": "filesystem",
          "key": "file",
          "value": "/:slug.html"
        }
      ],
      "permanent": false
    }
  ]
}
