{
  "rewrites": [
    {
      "source": "/home",
      "destination": "/index.html"
    },
    {
      "source": "/contato",
      "destination": "/contato.html"
    },
    {
      "source": "/:slug",
      "destination": "/:slug",
      "statusCode": 200
    }
  ],
  "redirects": [
    {
      "source": "/:slug",
      "destination": "/",
      "permanent": true
    }
  ]
}
